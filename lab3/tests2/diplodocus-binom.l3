// sum from i=0 to k
// of n+i choose n should equal (n+k+1 choose n+1)

int factorial(int n)
{
int i=0;
int count = 1;
for(i=n;i>0;i--)
{
count *=i;
}
return count;
}

int binom(int n, int k)
{
return (factorial(n))/(factorial(n-k)*factorial(k));
}

