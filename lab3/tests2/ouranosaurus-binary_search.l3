//test return 1029

// (from wikipedia)
int search(int target, int low, int high) {
    int mid = low + (high - low)/2;
    
    // you better hope we never hit this case
    if (low > high) {
        return -1;
    }
    else if (mid == target) {
        return mid;
    }
    else if (mid > target) {
        return search(target, low, mid-1);
    }
    else {
        return search(target, mid+1, high);
    }
}

int main() {
    return search(1029, 0, 1000000);
}
