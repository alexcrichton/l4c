/* sum all the factors of a number, use functions */

bool isDivisible(int x, int y){
	return (x % y) == 0;
}

int sumFactors(int x){
    int i;
    int sum = 0;

    for(i=1; i <= x; i++){
        if(isDivisible(x,i))
		sum += i;
    }
    return sum;
}

int main(){
    return sumFactors(68);
}