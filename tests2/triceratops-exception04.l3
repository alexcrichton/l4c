int function(int a) {
    int i;
    int res;

    res = 0;

    for (i = a; i >= 0; i--) {
        res /= i;
    }
    return res;
}

int tree(int a, int count) {
    int res = 0;
    if (count != 0) {
        res += a + tree(a, count - 1);
        function(a);
    }
    return a + res;
}

