int main ()
{
  int n;
  int sum;
  int i;
  int j;
  int k;

  n = 21;
  sum = 0;
  for (i = 2; i < n; i = ((i & 0x1f) + j + k) & 0xff) {
    sum += i;
    if (i % 17 == 0)
      break;
    else 
      j = 1;
    k = 1;
  }
  if (i % 17 != 0)
    return sum;
  else
    return 0;
}
