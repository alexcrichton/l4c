// Function decleration, type checking
int recursivesum(int x, int y);

int recursivesum(int x, int y)
{
  if( x == 0 && y == 0)
  {
    return 0;
  }

  return x + y + recursivesum(x/2, y/2);
}

int main () {
  int x;
  int y;

  x = 256;
  y = 256;
  
  return recursivesum(x, y);
}
