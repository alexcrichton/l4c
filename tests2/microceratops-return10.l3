/* test or, from previous class */

int fun_or(int x, int y)
{
  int result = 0;
  int i = 0;
  for (i = 0; i < 32; i++) {
    result *= 2;
    if (x < 0) {
      if (y < 0) {
        result += 1;
      }
    }
    x *= 2;
    y *= 2;
  }
  return result;

}

int main()
{
  int x = 124;
  int y = 23;

  return booltoint((fun_or(x, y) == (x & y)));
}
