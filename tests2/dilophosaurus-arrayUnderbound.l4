//test exception 11
//safe
// array access check
int main () {
  int SIZE = 255;
  int[] A = alloc_array(int, SIZE);
  int sum;
  int i;
  sum = 0;
  for (i = SIZE - 2; i >= 0; i--) {
    int j;
    for(j = i; j < SIZE; j++)
        A[j]++;
    sum += (A[j+1] + A[j] + A[j+1]) / 3; // A[0-1] will be under 0 !
  }
  return sum;
}
