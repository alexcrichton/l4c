//test return 6
//computes the number of 1's in the binary representation of 1337

bool one_at(int num, int pos)
{
  return ((num & (1 << pos)) > 0);
}

int num_ones(int x)
{
  int pos;
  int count = 0;
  for (pos = 0; pos <= 31; pos++)
  {
    if (one_at(x, pos)) count++;
  }
  return count;
}

int main()
{
  return num_ones(1337);
}
