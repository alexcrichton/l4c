//reverses a list in place
bool reverse(int n,int a,int b,int c,int d,int e)
{

if(a<b && b<c && c<d && d<e)
return true;

if(n==1)
return reverse(n-1,e,b,c,d,a);

if(n==2)
return reverse(n-1,a,d,c,b,e);

return false;

}


int main()
{
if(reverse(2,5,4,3,2,1))
return 1;


return 0;
}








