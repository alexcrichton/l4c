//test exception
// infinite mutual recursion

int bad1(int n);
int bad2(int n);
int bad3(int n);

int bad1(int n)
{
  if (n % 2 == 0)
    return bad2(2*n);
  else
    return bad3(n+1);
}

int bad2(int n)
{
  if (n % 5 == 0)
    return bad1(n);
  else
    return bad3(n-1);
}

int bad3(int n)
{
  return bad1(3*n);
}

int main()
{
  return bad1(1);
}
