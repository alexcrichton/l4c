int makechange(int amount){
    int ans;
    if(amount == 0){
        return 1;
    }
    ans = 0;
    if(amount >= 1){
        ans += makechange(amount-1);
    }
    if(amount >= 5){
        ans += makechange(amount-5);
    }
    if(amount >= 10){
        ans += makechange(amount-10);
    }
    if(amount >= 25){
        ans += makechange(amount-25);
    }
    return ans;
}

int main(){
    return makechange(37);
}
