//test return 3025
//computes the sum of the first 10 cubes while testing breaks and continues

int main()
{
	int sum = 0;

	int i;

	for(i = 1; i<11; i++)
	{
		sum += i*i*i;

		if(i%2==0)
		{
			sum--;
			continue;
		}
		
		if(i==5)
		{
			break;
		}

		sum ++;
	}

	i = 6;

	while(i<11)
	{
		sum += i*i*i;

		if(i%2==0)
		{
			sum --;
			i++;
			continue;
		}
		
		sum ++;
		i++;
	}

	sum ++;

	return sum;
}
