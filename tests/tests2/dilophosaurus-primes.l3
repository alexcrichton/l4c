// test a prime number
bool isPrime(int n){
	int i;
	for(i = 2; i < n / 2 + 1; i++){
		if(n % i == 0) return false;
	}
	return true;
}

// compute the sum of the 100 first prime numbers
int main(){
	int i = 2;
	int sum = 0;
	int c;
	for(c = 0;c < 100;i++){
		if(isPrime(i)){
			sum += i;
			c++;
		}
	}
	return sum;
}
