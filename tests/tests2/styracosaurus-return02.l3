//Tests mutual recursion using a function prototype

bool odd(int x);

bool even(int x)
{
	if(x == 0)
		return true;
	else
		return odd(x-1);
}

bool odd(int x)
{
	if(x == 0)
		return false;
	else
		return even(x-1);
}

int main()
{
	int x = 1024;
	if(even(x))
		return 1;
	else
		return 0;
}
