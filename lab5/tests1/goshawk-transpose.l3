// transposes a matrix (tests overflow arguments)

int transpose(int a, int b, int c,
              int d, int e, int f,
              int g, int h, int i)
{
    printint(a);
    printchar(0x20);
    printint(d);
    printchar(0x20);
    printint(g);
    printchar(0x0a);

    printint(b);
    printchar(0x20);
    printint(e);
    printchar(0x20);
    printint(h);
    printchar(0x0a);

    printint(c);
    printchar(0x20);
    printint(f);
    printchar(0x20);
    printint(i);
    printchar(0x0a);

    return 0;
}

int main()
{
    return transpose(3, 1, 4,
                     1, 5, 9,
                     2, 6, 5);
}
