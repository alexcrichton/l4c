int revBits(int in)
{
  int out;
  int bitPos;

  bitPos = 0;
  out = 0; 

  while( bitPos < 32) {
    bitPos += 1;
    out <<= 1;
    out |= (in & 0x1);
    in >>= 1;
  }
  return out;
}

int main () {
  int x = 242;
  printint(revBits(revBits(x)));
  printchar(32);
