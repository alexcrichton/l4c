//test return 0
// test some function summation

int sum(int i, int end, int s)
{
  if(i > end)
    return s;
  else
    return sum(i + 1, end, s + i);
}

int main()
{
  int i;
  int s = 0;

  if(sum(1, 100, 0) != 100 * 101 / 2)
    return -1;

  for(i = 1; i <= 999; i++)
    s = s + i;
  if(sum(1, 999, 0) != s)
    return -1;

  return 0;
}
