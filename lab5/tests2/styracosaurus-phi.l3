//Compute phi by using repeated fractions
int approx(int depth)
{
	int one = tofloat(1);
	int denom;

	if(depth == 0)
		return tofloat(1);

	denom = fadd(one, approx(depth - 1));
	return fdiv(one, denom);
}

//Print and return the 100th approximation
int main()
{
	int phi = approx(10000);
	int one = tofloat(1);
	int nl  = 10;

	printfloat(fadd(one, phi));
	printchar(nl);

	return phi;
}
