//test return 71
//tests that break only breaks out of the inner loop

int main() {
    int x;
    int y;
    int a = 1;

    for (x = 0; x < 10; x++) {
        for (y = 0; y < 10; y++) {
            if (a % 7 == 0)
                break; // break should only break out of the inner loop
            a++;
        }
        a++;
    }
    return a;
}
