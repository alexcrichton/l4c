//Print out the first 1000 square roots

typedef int float;

float sqrt(float s)
{
	float x = tofloat(1);
	int i;
	
	for(i=0; i<100; i++)
	{
		x = fadd(x, fdiv(s, x));
        x = fdiv(x, tofloat(2));
	}

	return x;
}


int main()
{
	int i;
	float s = 0;

	for(i=1; i<=1000; i++)
	{
		s = sqrt(tofloat(i));

		printint(i);
		printchar(58);	// colon
        printchar(32);
		printfloat(s);
		printchar(10);	// newline
	}

	return s;
}
