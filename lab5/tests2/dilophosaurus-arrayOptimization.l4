//test return 43
typedef int Sum;
typedef Sum Int_SIZE;
// strong reduction and loop invariants
int main () {
  int SIZE = 10;
  int[] A = alloc_array(Int_SIZE, SIZE);
  Sum sum;
  int i;
  sum = -2;
  for (i = 0; i < SIZE; i++) {
    int j;
    Sum s = 0;
    for(j = 0; j < SIZE; j++)
        A[j]++;
    s += A[i];
    sum += s - 1;
  }
  return sum;
}
