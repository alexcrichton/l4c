//test return 100
// the for loops may serve as value initialisers !
int main ()
{
  int n;
  int sum;
  int i;
  int j;
  int s;

  n = 3;
  sum = 0;
  for (i = 1; i <= n; i+=1) {
    for (j = 1; j <= n; j+=1) {
      for(s = 5; s <= 0xf; s++) {}
      sum = sum + i*j;
      if (i != j) continue;	// continue inner loop, not outer
      sum = sum + i*j;		// count diag twice
    }
  }
  for (i = 1; i <= n; i+=1) {
    j = 1;
    while (j <= n) {
      for(s = 0xf; s > 1; s--) {}
      sum = sum + i*j;
      j += s;
      if (i+1 != j) continue;	// continue inner loop, not outer
      sum = sum + i*(j-1);	// count diag twice
    }
  }

  return sum;
}
