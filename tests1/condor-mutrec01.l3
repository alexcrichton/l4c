//test return 0
bool even(int i);
bool odd(int i);

bool even(int i) {
    if (i == 0)
       return true;
    else return odd(i - 1);
}

bool odd (int i) {
    if (i == 0)
       return false;
    else return even(i - 1);
}

int toInt (bool tf) {
    return tf ? 1 : 0 ;
}

int main () {
    int x = 5;
    return toInt(even(x));
}