//test exception 11

struct BadList;
typedef struct BadList * BadList;

struct BadList {
       int val;
       BadList next;
};

BadList build(int i) {
	if (i == 0) return NULL;
	BadList curr = alloc(struct BadList);
	curr->val = i;
	curr->next = build(i-1);
	return curr;
}

int getNth(BadList curr, int i) {
	if (i <= 0) return curr->val;
	return getNth(curr->next, i-1);
}

int main () {
    int LEN = 1000;
    BadList [] haha = alloc_array(BadList, LEN);

    for (int i = 0 ; i < LEN; i ++)
  	haha[i] = build(i+1);
    for (int i = 0; i < LEN; i++)
        getNth(haha[i],i);

    return haha[LEN+1]->val;
}