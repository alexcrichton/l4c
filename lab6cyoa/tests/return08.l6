//test return 18

class C1 {
private:
  int n1;
public:
  C1();
  int f1();
  int ans();
};

class C2 extends C1 {
private:
  int n2;
public:
  C2();
  int f2();
  int ans();
};

class C3 extends C2 {
private:
  int n3;
public:
  C3();
  int f3();
  int ans();
};

class C4 extends C3 {
private:
  int n4;
public:
  C4();
  int f4();
  int ans();
};

class C5 extends C4 {
private:
  int n5;
public:
  C5();
  int f5();
  int ans();
};

C1::C1() {
  n1 = 1;
}
int C1::f1() {
  return n1;
}
int C1::ans() {
  return this->f1();
}

C2::C2() {
  n2 = 2;
}
int C2::f2() {
  return n2 + this->f1();
}
int C2::ans() {
  return this->f2();
}

C3::C3() {
  n3 = 3;
}
int C3::f3() {
  return n3 + this->f2();
}
int C3::ans() {
  return this->f3();
}

C4::C4() {
  n4 = 4;
}
int C4::f4() {
  return n4 + this->f3();
}
int C4::ans() {
  return this->f4();
}

C5::C5() {
  n5 = 5;
}
int C5::f5() {
  return n5 + this->f4();
}
int C5::ans() {
  return this->f5();
}

int main() {
  C1 *c1 = new C1();
  C1 *c2 = new C2();
  C1 *c3 = new C3();
  C1 *c4 = new C4();
  C1 *c5 = new C5();
  return c1->ans() + c2->ans() + c3->ans() + c4->ans() + c5->ans();
}
